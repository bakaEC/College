package demo;
import java.util.*;

public class Demo extends p{
	public static void main(String[] args) {
		Scanner sc= new Scanner(System.in);

		while(true) {
		switch(sc.nextInt()) {
		case 1:
			Cat c=new Cat();
			println("I am a cat");
			c.eat();
			c.ClimbWays();
			c.eatten();
			c.live();
			break;
		case 2:
			Fish f=new Fish();
			println("I am a cat");
			f.eat();
			f.ClimbWays();
			f.eatten();
			f.live();
			break;
		
		}
		}
	}
}
	


